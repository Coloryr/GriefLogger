{
  "schemaVersion": 1,
  "id": "grieflogger",
  "version": "${version}",
  "name": "GriefLogger",
  "description": "A mod that logs all player interactions with blocks and entities and stores them in a database.",
  "authors": [
    "DAQEM"
  ],
  "contact": {
    "homepage": "https://daqem.com/",
    "sources": "https://github.com/DAQEM/GriefLogger"
  },
  "license": "Apache-2.0",
  "icon": "assets/grieflogger/icon.png",
  "environment": "*",
  "entrypoints": {
    "server": [
      "com.daqem.grieflogger.fabric.GriefLoggerFabricServer"
    ]
  },
  "mixins": [
    "grieflogger.mixins.json",
    "grieflogger-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=1.19.2",
    "architectury": ">=6.5.85",
    "supermartijn642configlib": ">=${config_library_version}"
  }
}